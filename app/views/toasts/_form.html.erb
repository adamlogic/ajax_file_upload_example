<%= error_messages_for :toast %>
<% form_for @toast, :html => {:multipart => true} do |f| %>
  <%= hidden_field_tag :format, 'js' %>
  <p>
    <%= f.label :bread %><br />
    <%= f.text_field :bread %>
  </p>
  <p>
    <%= f.label :photo %><br />
    <span class="attach_file">
      <%= image_tag 'attach.png' %>
      <%= f.file_field :photo %>
    </span>
  </p>
  <p><%= f.submit "Push it down" %></p>
<% end %>
